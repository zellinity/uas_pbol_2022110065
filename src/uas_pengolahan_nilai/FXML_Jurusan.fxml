<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="uas_pengolahan_nilai.FXML_JurusanController">
    <TableView fx:id="tableJurusan">
        <columns>
            <TableColumn fx:id="colKdJurusan" text="Kode Jurusan" />
            <TableColumn fx:id="colJurusan" text="Jurusan" />
        </columns>
    </TableView>
    <HBox spacing="10" alignment="CENTER">
        <Button text="Tambah" fx:id="btnTambah" />
        <Button text="Edit" fx:id="btnEdit" />
        <Button text="Hapus" fx:id="btnHapus" />
    </HBox>
</VBox>
